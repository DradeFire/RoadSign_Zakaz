        android:ems="10"
        android:hint="9"
        android:imeOptions="actionNext"
        android:inputType="text"
        android:maxLength="1"
        android:nextFocusForward="@+id/edR3Num3"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/edR3Num1"
