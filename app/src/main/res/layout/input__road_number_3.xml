        android:layout_height="52dp"
        android:backgroundTint="#00FFFFFF"
        android:ems="10"
        android:hint="9"
        android:inputType="text"
        android:maxLength="1"
        android:nextFocusForward="@+id/edR3Num6"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
