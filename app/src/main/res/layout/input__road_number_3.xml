<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="270dp"
    android:layout_height="60dp"
    android:background="@drawable/f2">

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/edR3Text1"
        android:layout_width="32dp"
        android:layout_height="52dp"
        android:layout_marginStart="6dp"
        android:backgroundTint="#00FFFFFF"
        android:ems="10"
        android:hint="A"
        android:imeOptions="actionNext"
        android:inputType="text"
        android:maxLength="1"
        android:nextFocusForward="@+id/edR3Text2"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/edR3Text2"
        android:layout_width="32dp"
        android:layout_height="52dp"
        android:backgroundTint="#00FFFFFF"
        android:ems="10"
        android:hint="A"
        android:imeOptions="actionNext"
        android:inputType="text"
        android:maxLength="1"
        android:nextFocusForward="@+id/edR3Num1"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/edR3Text1" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/edR3Num1"
        android:layout_width="28dp"
        android:layout_height="0dp"
        android:backgroundTint="#00FFFFFF"
        android:ems="10"
        android:hint="9"
        android:imeOptions="actionNext"
        android:inputType="text"
        android:maxLength="1"
        android:nextFocusForward="@+id/edR3Num2"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/edR3Text2"
